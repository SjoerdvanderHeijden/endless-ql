stylesheet taxOfficeExample {
    default boolean widget radio

    page Personalia {
        section "Personalia" {

            expression name
            expression name

            expression age
            expression grade
        }
    }

    page Housing {
        section "Buying" {
            expression hasBoughtHouse
                widget checkbox
        }

        section "Loaning" {
            expression hasMaintLoan
        }
    }

    page Selling {
        section "Selling" {
            expression hasSoldHouse
                widget dropdown
            expression blockSold
        }
    }
}

