<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nl.uva.se.sc.niro.gui.controller.QLFormController">
   <top>
       <VBox alignment="CENTER" BorderPane.alignment="CENTER">
           <Label fx:id="formName" contentDisplay="CENTER" text="FormName" textAlignment="CENTER" BorderPane.alignment="CENTER">
             <font>
                <Font name="System Bold" size="24.0" />
             </font>
         </Label>
            <Label fx:id="pageName" text="Page Name">
                <font>
                    <Font size="18.0"/>
                </font>
                <padding>
                    <Insets top="10.0"/>
                </padding>
            </Label>
            <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
          <padding>
                <Insets bottom="10.0" top="5.0"/>
            </padding>
        </VBox>
   </top>
   <bottom>
      <VBox>
            <BorderPane fx:id="navigationBar" prefWidth="200.0">
                <left>
                    <Button fx:id="previous" onAction="#previousPage" text="Back"/>
                </left>
                <right>
                    <Button fx:id="next" onAction="#nextPage" text="Next"/>
                </right>
                <padding>
                    <Insets left="10.0" right="10.0"/>
                </padding>
            </BorderPane>
            <HBox prefWidth="200.0" spacing="10.0" BorderPane.alignment="CENTER">

            <Button mnemonicParsing="false" onAction="#quitApplication" text="Quit" />
            <Button mnemonicParsing="false" onAction="#cancel" text="Cancel" />
            <Button mnemonicParsing="false" onAction="#saveData" text="Save" />
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </VBox>
    </bottom>
   <center>
      <ScrollPane>
          <VBox fx:id="questionArea" maxWidth="1.7976931348623157E308" minWidth="-Infinity">
         <padding>
            <Insets left="20.0" right="20.0" />
         </padding></VBox></ScrollPane>
   </center>
</BorderPane>
