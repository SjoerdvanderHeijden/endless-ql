<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<GridPane fx:id="mainGrid" alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.141"
          xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.uva.sea.ql.gui.QuestionController">
    <padding>
        <Insets bottom="40" left="40" right="40" top="40"/>
    </padding>
    <Button onAction="#addCheckBox" text="Add check box" GridPane.rowIndex="0"/>
    <Button onAction="#addTextField" text="Add text field" GridPane.rowIndex="1"/>
    <Button onAction="#addLabel" text="Add label" GridPane.rowIndex="2"/>
    <Button onAction="#addNewQuestion" text="Add a new question" GridPane.rowIndex="3"/>
    <VBox fx:id="vBox" prefHeight="200.0" prefWidth="100.0" GridPane.rowIndex="4"/>
    <columnConstraints>
        <ColumnConstraints/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints/>
        <RowConstraints/>
        <RowConstraints/>
        <RowConstraints/>
        <RowConstraints/>
    </rowConstraints>
</GridPane>
